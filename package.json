{
	"name": "equip-gen",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"dep": "gh-pages -d build -t true"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.5.28",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"typescript": "^5.6.2",
		"vite": "^5.4.6"
	},
	"type": "module",
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"vite": "5"
			}
		}
	},
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.1.2"
	}
}